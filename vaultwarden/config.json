{
  "arch": [
    "amd64",
    "aarch64"
  ],
  "boot": "auto",
  "description": "用 Rust 编写的 Bitwarden 服务器 API 的替代实现，与上游 Bitwarden 客户端*兼容，非常适合自托管部署，因为在运行官方资源密集型服务可能并不理想的情况下",
  "environment": {
    "DATA_FOLDER": "/config"
  },
  "map": [
    "addon_config:rw"
  ],
  "image": "ghcr.nju.edu.cn/dani-garcia/vaultwarden",
  "version": "1.34.3",
  "name": "Vaultwarden",
  "options": {},
  "ports": {
    "80/tcp": "7277"
  },
  "ports_description": {
    "80/tcp": "Web interface"
  },
  "schema": {},
  "slug": "vaultwarden",
  "startup": "services",
  "upstream": "1.34.3",
  "url": "https://ghfast.top/github.com/jdeath/homeassistant-addons",
  "webui": "[PROTO:ssl]://[HOST]:[PORT:80]",
  "ingress": true,
  "ingress_port": 80
}
